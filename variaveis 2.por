programa {
  funcao inicio() {
    //Declaração de variaveis
    cadeia nome
    inteiro idade
    real altura

    //atribuição com comando leia
    escreva ("Digite o seu nome: ")
    leia (nome)
    escreva ("Digite a sua idade: ")
    leia (idade)
    escreva ("Digite a sua altura: ")
    leia (altura)

    escreva("Nome: ", nome, "\nidade: ", idade, "\naltura: ", altura)
  }
}
